package uld.sdk.unite;

public class RechargeResult {
	int channelId = 0;
	String orderId = null;
	int orderType = 0;
	String orderMsg = null;
	float payAccount = 0;

	/**
	 * @return the orderId
	 */
	public String getOrderId() {
		return orderId;
	}
	/**
	 * @return the payAccount
	 */
	public float getPayAccount() {
		return payAccount;
	}
	/**
	 * @param payAccount the payAccount to set
	 */
	public void setPayAccount(float payAccount) {
		this.payAccount = payAccount;
	}
	/**
	 * @param orderId the orderId to set
	 */
	public void setOrderId(String orderId) {
		this.orderId = orderId;
	}
	/**
	 * @return the orderType
	 */
	public int getOrderType() {
		return orderType;
	}
	/**
	 * @param orderType the orderType to set 订单状态， 0 充值成功， 1 充值已提交，2 充值失败，3 尚未充值
	 */
	public void setOrderType(int orderType) {
		this.orderType = orderType;
	}
	/**
	 * @return the odderMsg
	 */
	public String getOrderMsg() {
		return orderMsg;
	}
	public RechargeResult() {
		
	}
	/**
	 * @param odderMsg the odderMsg to set
	 */
	public void setOrderMsg(String orderMsg) {
		this.orderMsg = orderMsg;
	}
	/**
	 * @return the channelId
	 */
	public int getChannelId() {
		return channelId;
	}
	/**
	 * @param channelId the channelId to set
	 */
	public void setChannelId(int channelId) {
		this.channelId = channelId;
	}		
	
}
