package uld.sdk.unite;

public class LoginResult {
	String channelUserId = null;
	String channelUserName = null;

	Boolean isLogin = false;
	String loginErrorMsg = null;

	int channelId = 0;
	String channelName = "";
	
	String timeSign = null;

	String sign = null;

	/**
	 * @return the isLogin
	 */
	public Boolean getIsLogin() {
		return isLogin;
	}

	/**
	 * @param isLogin
	 *        the isLogin to set
	 */
	public void setIsLogin(Boolean isLogin) {
		this.isLogin = isLogin;
	}

	/**
	 * @return the loginErrorMsg
	 */
	public String getLoginErrorMsg() {
		return loginErrorMsg;
	}

	/**
	 * @param loginErrorMsg
	 *        the loginErrorMsg to set
	 */
	public void setLoginErrorMsg(String loginErrorMsg) {
		this.loginErrorMsg = loginErrorMsg;
	}

	/**
	 * @return 获取用户注册时的渠道编号
	 */
	public int getChannelId() {
		return channelId;
	}

	/**
	 * @param 设置用户注册时的渠道编号
	 */
	public void setChannelId(int channelId) {
		this.channelId = channelId;
	}

	/**
	 * @return 获取用户注册时的渠道名称
	 */
	public String getChannelName() {
		return channelName;
	}

	/**
	 * @param 设置用户注册时的渠道名称
	 */
	public void setChannelName(String channelName) {
		this.channelName = channelName;
	}

	/**
	 * @return 获取登录验证串
	 */
	public String getSign() {
		return sign;
	}

	/**
	 * @param 设置登录验证串
	 */
	public void setSign(String sign) {
		this.sign = sign;
	}

	/**
	 * @return the channelUserId
	 */
	public String getChannelUserId() {
		return channelUserId;
	}

	/**
	 * @param channelUserId
	 *        the channelUserId to set
	 */
	public void setChannelUserId(String channelUserId) {
		this.channelUserId = channelUserId;
	}

	/**
	 * @return the channelUserName
	 */
	public String getChannelUserName() {
		return channelUserName;
	}

	/**
	 * @param channelUserName
	 *        the channelUserName to set
	 */
	public void setChannelUserName(String channelUserName) {
		this.channelUserName = channelUserName;
	}
	
	/**
	 * @return 获取时间加密
	 */
	public String getTimeSign() {
		return timeSign;
	}

	/**
	 * @param 设置时间加密
	 */
	public void setTimeSign(String timeSign) {
		this.timeSign = timeSign;
	}
}
