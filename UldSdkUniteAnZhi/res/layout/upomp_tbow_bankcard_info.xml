			<!-- 界面下面提示文字 -->
			<RelativeLayout
				android:id="@+id/layout_tip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="12.0dip"
				android:layout_marginTop="30.0dip"
				android:layout_marginRight="12.0dip"
				android:layout_below="@+id/tbow_pay_7F070294"
				android:layout_centerHorizontal="true"
				>
				<ImageView
					android:textColor="@color/anzhi_cFF000000"
					android:id="@+id/img_7F0700FF"
					android:background="@drawable/upomp_tbow_tips_title"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					>
				</ImageView>
				<ImageView
						android:id="@+id/imageView_tbow_pay_tips_title_youqingtishi"
						android:background="@drawable/upomp_tbow_tips_title2"
						android:layout_width="fill_parent"
						android:layout_height="wrap_content"
						android:layout_below="@+id/img_7F0700FF"
						>
					</ImageView>
				<RelativeLayout
					android:id="@+id/layout_tip_content_7F070100"
					android:background="@drawable/upomp_tbow_tips_bg"
					android:paddingBottom="10.0dip"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/imageView_tbow_pay_tips_title_youqingtishi"
					>
					<TextView
						android:textSize="14.0sp"
						android:textColor="@color/anzhi_cFF666666"
						android:id="@+id/tv_tip"
						android:layout_width="fill_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10.0dip"
						android:layout_marginRight="10.0dip"
						>
					</TextView>
				</RelativeLayout>
				<ImageView
					android:textColor="@color/anzhi_cFF000000"
					android:id="@+id/img_7F070102"
					android:background="@drawable/upomp_tbow_tips_bot"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/layout_tip_content_7F070100"
					>
				</ImageView>
			</RelativeLayout>
		</RelativeLayout>
	</ScrollView>
</LinearLayout>
