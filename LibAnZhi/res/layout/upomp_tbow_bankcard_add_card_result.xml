<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/layout_add_result"
	android:background="@color/anzhi_c00000000"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	>
	<ScrollView
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		>
		<RelativeLayout
			android:layout_width="fill_parent"
			android:layout_height="fill_parent"
			>
			<RelativeLayout
				android:id="@+id/layout_add_success"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="20.0dip"
				>
				<RelativeLayout
					android:id="@+id/layout_tv_add_success"
					android:background="@drawable/upomp_tbow_card_info_title"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="15.0dip"
					android:layout_marginTop="20.0dip"
					android:layout_marginRight="15.0dip"
					>
					<TextView
						android:textSize="20.0sp"
						android:textColor="@color/anzhi_cFFFFFFFF"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10.0dip"
						android:text="@string/yl_add_yl_success"
						android:layout_centerVertical="true"
						>
					</TextView>
				</RelativeLayout>
				<RelativeLayout
					android:id="@+id/layout_card_info"
					android:background="@drawable/upomp_tbow_card_info_bg"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="15.0dip"
					android:layout_marginRight="15.0dip"
					android:layout_below="@+id/layout_tv_add_success"
					>
					
				    <RelativeLayout
						android:id="@+id/layout_bank_name"
						android:layout_width="fill_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="3.0dip"
						>
						<RelativeLayout
							android:id="@+id/layout_tv_bank_name"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							>
							<TextView
								android:textSize="15.0sp"
								android:textColor="@color/anzhi_cFF000000"
								android:id="@+id/tv_bank_name"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_marginLeft="10.0dip"
								android:text="@string/yl_fa_ka_h"
								>
							</TextView>
						</RelativeLayout>
						<RelativeLayout
							android:id="@+id/layout_tv_real_bank_name"
							android:layout_width="fill_parent"
							android:layout_height="wrap_content"
							android:layout_marginLeft="5.0dip"
							android:layout_toRightOf="@+id/layout_tv_card_type"
							>
							<TextView
								android:textSize="15.0sp"
								android:textColor="@color/anzhi_cFF000000"
								android:id="@+id/tv_real_bank_name"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								>
							</TextView>
						</RelativeLayout>
					</RelativeLayout>
				    
					<RelativeLayout
						android:id="@+id/layout_card_type"
						android:layout_width="fill_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="3.0dip"
						android:layout_below="@+id/layout_bank_name"
						>
						<RelativeLayout
							android:id="@+id/layout_tv_card_type"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							>
							<TextView
								android:textSize="15.0sp"
								android:textColor="@color/anzhi_cFF000000"
								android:id="@+id/tv_card_type"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_marginLeft="10.0dip"
								android:text="@string/yl_card_stlye"
								>
							</TextView>
						</RelativeLayout>
						<RelativeLayout
							android:id="@+id/layout_tv_real_card_type"
							android:layout_width="fill_parent"
							android:layout_height="wrap_content"
							android:layout_marginLeft="5.0dip"
							android:layout_toRightOf="@+id/layout_tv_card_type"
							>
							<TextView
								android:textSize="15.0sp"
								android:textColor="@color/anzhi_cFF000000"
								android:id="@+id/tv_real_card_type"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								>
							</TextView>
						</RelativeLayout>
					</RelativeLayout>
					<RelativeLayout
						android:id="@+id/layout_card_num"
						android:layout_width="fill_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="3.0dip"
						android:layout_below="@+id/layout_card_type"
						>
						<RelativeLayout
							android:id="@+id/layout_tv_card_num"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							>
							<TextView
								android:textSize="15.0sp"
								android:textColor="@color/anzhi_cFF000000"
								android:id="@+id/tv_card_num"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_marginLeft="10.0dip"
								android:text="@string/yl_card_num"
								>
							</TextView>
						</RelativeLayout>
						<RelativeLayout
							android:id="@+id/layout_tv_real_card_num"
							android:layout_width="fill_parent"
							android:layout_height="wrap_content"
							android:layout_marginLeft="5.0dip"
							android:layout_toRightOf="@+id/layout_tv_card_num"
							>
							<TextView
								android:textSize="15.0sp"
								android:textColor="@color/anzhi_cFF000000"
								android:id="@+id/tv_real_card_num"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								>
							</TextView>
						</RelativeLayout>
					</RelativeLayout>
					<RelativeLayout
						android:id="@+id/layout_phone_num"
						android:layout_width="fill_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="3.0dip"
						android:layout_below="@+id/layout_card_num"
						>
						<RelativeLayout
							android:id="@+id/layout_tv_phone_num"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							>
							<TextView
								android:textSize="15.0sp"
								android:textColor="@color/anzhi_cFF000000"
								android:id="@+id/tv_phone_num"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_marginLeft="10.0dip"
								android:text="@string/yl_tel_num"
								>
							</TextView>
						</RelativeLayout>
						<RelativeLayout
							android:id="@+id/layout_tv_real_phone_num"
							android:layout_width="fill_parent"
							android:layout_height="wrap_content"
							android:layout_marginLeft="5.0dip"
							android:layout_toRightOf="@+id/layout_tv_phone_num"
							>
							<TextView
								android:textSize="15.0sp"
								android:textColor="@color/anzhi_cFF000000"
								android:id="@+id/tv_real_phone_num"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								>
							</TextView>
						</RelativeLayout>
					</RelativeLayout>
				</RelativeLayout>
				<RelativeLayout
					android:background="@drawable/upomp_tbow_card_info_bot"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="15.0dip"
					android:layout_marginRight="15.0dip"
					android:layout_below="@+id/layout_card_info"
					>
				</RelativeLayout>
			</RelativeLayout>
			<RelativeLayout
				android:gravity="bottom"
				android:id="@+id/rl_add_card_failure"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="20.0dip"
				android:layout_below="@+id/layout_add_success"
				>
				<RelativeLayout
					android:id="@+id/rl_tv_failure"
					android:background="@drawable/upomp_tbow_card_info_title"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="15.0dip"
					android:layout_marginTop="20.0dip"
					android:layout_marginRight="15.0dip"
					android:layout_below="@+id/layout_add_success"
					>
					<TextView
						android:textSize="20.0sp"
						android:textColor="@color/anzhi_cFFFFFFFF"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10.0dip"
						android:text="@string/yl_add_yl_faild"
						android:layout_centerVertical="true"
						>
					</TextView>
				</RelativeLayout>
				<RelativeLayout
					android:id="@+id/rl_tv_failure_reason"
					android:background="@drawable/upomp_tbow_card_info_bg"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="15.0dip"
					android:layout_marginRight="15.0dip"
					android:layout_below="@+id/rl_tv_failure"
					>
					<TextView
						android:textSize="15.0sp"
						android:textColor="@color/anzhi_cFF000000"
						android:id="@+id/tv_failure_reason"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10.0dip"
						android:layout_marginTop="3.0dip"
						android:text="@string/yl_faild_reson"
						>
					</TextView>
				</RelativeLayout>
				<RelativeLayout
					android:id="@+id/rl_img_7F0700B9"
					android:background="@drawable/upomp_tbow_card_info_bot"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="15.0dip"
					android:layout_marginRight="15.0dip"
					android:layout_below="@+id/rl_tv_failure_reason"
					>
				</RelativeLayout>
			</RelativeLayout>
			<RelativeLayout
				android:gravity="bottom"
				android:id="@+id/rl_two_buttons"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="20.0dip"
				android:layout_below="@+id/rl_add_card_failure"
				>
				<Button
					android:textSize="20.0sp"
					android:textColor="@color/anzhi_cFFFFFFFF"
					android:id="@+id/btn_back_card_list"
					android:background="@drawable/upomp_tbow_btn_enter"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="15.0dip"
					android:text="@string/yl_return_list"
					android:layout_alignParentLeft="true"
					>
				</Button>
				<Button
					android:textSize="20.0sp"
					android:textColor="@color/anzhi_cFFFFFFFF"
					android:id="@+id/btn_binding_card"
					android:background="@drawable/upomp_tbow_btn_esc"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginRight="15.0dip"
					android:text="@string/yl_goon_bind"
					android:layout_alignParentRight="true"
					>
				</Button>
			</RelativeLayout>
		</RelativeLayout>
	</ScrollView>
</LinearLayout>
