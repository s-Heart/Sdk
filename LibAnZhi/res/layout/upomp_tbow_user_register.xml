					android:layout_height="wrap_content"
					android:layout_marginLeft="15.0dip"
					android:layout_marginRight="15.0dip"
					>
					<RelativeLayout
						android:id="@+id/layout_in_tv_username"
						style="@style/RelativeLayout_Text2"
						android:layout_centerVertical="true"
						>
						<TextView
							android:textSize="15.0sp"
							android:textColor="@color/anzhi_cFF000000"
							android:gravity="center_vertical"
							android:layout_alignParentRight="true"
							android:layout_width="wrap_content"
							android:layout_height="40.0dip"
							android:layout_marginLeft="10.0dip"
							android:text="@string/yl_user"
							>
						</TextView>
					</RelativeLayout>
					<RelativeLayout
						android:id="@+id/layout_in_et_username"
						android:layout_width="fill_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="5.0dip"
						android:layout_marginRight="0.0dip"
						android:layout_toRightOf="@+id/layout_in_tv_username"
						>
						<EditText
							android:textSize="15.0sp"
							android:background="@color/anzhi_c00000000"
							android:textColor="@color/anzhi_cFF000000"
