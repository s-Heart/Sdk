					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="15.0dip"
					android:layout_marginTop="3.0dip"
					android:layout_marginRight="15.0dip"
					android:layout_below="@+id/layout_in_username"
					>
					<RelativeLayout
						android:id="@+id/layout_in_tv_pw"
						style="@style/RelativeLayout_Text2"
						android:layout_centerVertical="true"
						>
						<TextView
							android:textSize="15.0sp"
							
							android:textColor="@color/anzhi_cFF000000"
							android:gravity="center_vertical"
						android:layout_alignParentRight="true"
							android:layout_width="wrap_content"
							android:layout_height="40.0dip"
							android:layout_marginLeft="10.0dip"
							android:text="@string/yl_pswb"
							>
						</TextView>
					</RelativeLayout>
					<RelativeLayout
						android:id="@+id/layout_in_button_pw"
						android:layout_width="fill_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="5.0dip"
						android:layout_marginRight="5.0dip"
						android:layout_toRightOf="@+id/layout_in_tv_pw"
						>
						<Button
							android:textColor="@color/anzhi_cFF000000"
							android:background="@color/anzhi_c00000000"
							android:gravity="center_vertical"
							android:id="@+id/button_pw"
							android:layout_width="fill_parent"
							android:layout_height="40.0dip"
							android:hint="@string/yl_6_chars"
							android:textSize="15.0sp"
							>
						</Button>
					</RelativeLayout>
				</RelativeLayout>
				<RelativeLayout
					android:id="@+id/layout_pw_confirm"
					android:background="@drawable/upomp_tbow_checkbox"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="15.0dip"
					android:layout_marginTop="3.0dip"
					android:layout_marginRight="15.0dip"
					android:layout_below="@+id/layout_pw"
					>
					<RelativeLayout
						android:id="@+id/layout_in_tv_pw_confirm"
						style="@style/RelativeLayout_Text2"
						android:layout_centerVertical="true"
						>
						<TextView
							android:textSize="15.0sp"
							android:textColor="@color/anzhi_cFF000000"
							android:gravity="center_vertical"
						android:layout_alignParentRight="true"
							android:layout_width="wrap_content"
							android:layout_height="40.0dip"
							android:layout_marginLeft="10.0dip"
							android:text="@string/yl_queren_pswb"
							>
						</TextView>
					</RelativeLayout>
					<RelativeLayout
						android:id="@+id/layout_in_button_pw_confirm"
						android:layout_width="fill_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="5.0dip"
						android:layout_marginRight="5.0dip"
						android:layout_toRightOf="@+id/layout_in_tv_pw_confirm"
						>
						<Button
							android:textColor="@color/anzhi_cFF000000"
							android:background="@color/anzhi_c00000000"
							android:gravity="center_vertical"
							android:id="@+id/button_pw_confirm"
							android:layout_width="fill_parent"
							android:layout_height="40.0dip"
							android:hint="@string/yl_6_chars"
							android:textSize="15.0sp"
							>
						</Button>
					</RelativeLayout>
				</RelativeLayout>
				<RelativeLayout
					android:id="@+id/layout_phonenum"
					android:background="@drawable/upomp_tbow_checkbox"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="15.0dip"
					android:layout_marginTop="3.0dip"
					android:layout_marginRight="15.0dip"
					android:layout_below="@+id/layout_pw_confirm"
					>
					<RelativeLayout
						android:id="@+id/layout_in_tv_phonenum"
						style="@style/RelativeLayout_Text2"
						android:layout_centerVertical="true"
						>
						<TextView
							android:textSize="15.0sp"
							android:textColor="@color/anzhi_cFF000000"
							android:gravity="center_vertical"
						android:layout_alignParentRight="true"
							android:layout_width="wrap_content"
							android:layout_height="40.0dip"
							android:layout_marginLeft="10.0dip"
							android:text="@string/yl_tel_num2"
