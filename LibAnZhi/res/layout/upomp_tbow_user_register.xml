						<EditText
							android:textSize="15.0sp"
							android:background="@color/anzhi_c00000000"
							android:textColor="@color/anzhi_cFF000000"
							android:id="@+id/et_Security_text_answer"
							android:layout_width="fill_parent"
							android:layout_height="40.0dip"
							android:hint="@string/yl_2_dfg"
							android:singleLine="true"
							android:maxLength="16"
							>
						</EditText>
					</RelativeLayout>
				</RelativeLayout>
				<RelativeLayout
					android:id="@+id/layout_verify_num"
					android:background="@drawable/upomp_tbow_checkbox"
