								android:background="@color/anzhi_c00000000"
								android:gravity="center_vertical"
								android:id="@+id/button_Security_text"
								android:layout_width="fill_parent"
								android:layout_height="35.0dip"
								android:hint="@string/yl_shezhi_tishi_wenti"
								android:singleLine="true"
								>
							</Button>
						</RelativeLayout>
					</RelativeLayout>
				</RelativeLayout>
				<RelativeLayout
					android:id="@+id/layout_userdefined_Security_text"
					android:background="@drawable/upomp_tbow_checkbox"
