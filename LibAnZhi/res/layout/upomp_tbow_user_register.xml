							android:text="@string/yl_anquan_danan"
							>
						</TextView>
					</RelativeLayout>
					<RelativeLayout
						android:id="@+id/layout_in_et_Security_text_answer"
						android:layout_width="fill_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="5.0dip"
						android:layout_marginRight="5.0dip"
						android:layout_toRightOf="@+id/layout_in_tv_Security_text_answer"
