<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:background="@color/anzhi_c00000000"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent">
	<ScrollView
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:layout_marginTop="10.0dip"
		android:fadingEdge="none"
		>
		<RelativeLayout
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		>
			
			<!-- 登录和注册按钮 -->
			
		<LinearLayout
				android:id="@+id/layout_twobutton_title"
				android:orientation="horizontal"
				android:layout_width="fill_parent"
				android:layout_height="fill_parent"
				android:layout_marginTop="0dip"
				android:gravity="center_vertical"
    	>        
 			<Button 
 					android:id="@+id/button_login"
 					android:background="@drawable/upomp_tbow_sub10_default"
					android:textSize="17.0sp"
					android:textColor="@color/anzhi_cFFFFFFFF"
					android:layout_width="0px"
					android:layout_height="wrap_content"
					android:text="@string/yl_load"
					android:layout_weight="1"
					android:layout_marginLeft="15.0dip"
        	/>  
 			<Button 
					android:textSize="17.0sp"
					android:background="@drawable/upomp_tbow_sub11_on"
					android:textColor="@color/anzhi_cFFFFFFFF"
					android:layout_width="0px"
					android:layout_height="wrap_content"
					android:text="@string/yl_zhuce"
					android:layout_weight="1"
					android:layout_marginRight="15.0dip"
        	/> 
	</LinearLayout>
	<!--  -->
	<RelativeLayout
